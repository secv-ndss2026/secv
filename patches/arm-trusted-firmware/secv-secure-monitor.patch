+static void XRDC_WAIT(void) {
+    volatile uint32_t x = 0x1000000;
+    while(x > 0){            
+        ASM_KEYWORD("nop ");
+        ASM_KEYWORD("nop "); 
+        ASM_KEYWORD("nop "); 
+        ASM_KEYWORD("nop "); 
+        ASM_KEYWORD("nop "); 
+        ASM_KEYWORD("nop "); 
+        ASM_KEYWORD("nop "); 
+        ASM_KEYWORD("nop "); 
+        ASM_KEYWORD("nop "); 
+        ASM_KEYWORD("nop "); 
+        ASM_KEYWORD("nop "); 
+        ASM_KEYWORD("nop "); 
+        x--;                 
+    }                       
+}
