+*
+*/
+static uint32 Xrdc_Get_Priviledge_Attribute(Xrdc_Ip_PriviledgedAttributeType eXrdcPriviledge)
+{
+    uint32 Ret;
+
+    if (eXrdcPriviledge == XRDC_USER_MODE)
